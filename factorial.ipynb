   "source": [
    "def factorial(N):\n",
    "    fact=1\n",
    "    for i in range(1,N+1):\n",
    "        fact*=i\n",
    "    print(fact)\n",
    "    \n",
    "    \n",
    "factorial(10)"
   ]
  }
